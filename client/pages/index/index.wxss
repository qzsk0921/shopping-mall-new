  font-weight: bold;
  color: #FA4953;
}

.section6 .r {
  padding: 24rpx 5rpx 24rpx 0rpx;
  display: flex;
  align-items: center;
  overflow: scroll;
}

.section6 .r .item {
  width: 156rpx;
  height: 204rpx;
  background: #F7F7F7;
  border-radius: 10rpx;
}

.section6 .r .item:not(:first-child) {
  margin-left: 14rpx;
}

.section6 .r .item .icon-newmarket {
  margin: 10rpx 8rpx 0;
  width: 140rpx;
  height: 140rpx;
  border-radius: 8rpx;
}

.section6 .r .item .l2 {
  text-align: center;
  margin-top: 12rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #FF2C38;
}

