/* pages/index/index.wxss */
.container-index {}

.content {}

.content>.cont {
  padding-bottom: 60rpx;
  overflow: auto;
  position: relative;
}

.bg {
  position: absolute;
  top: -116rpx;
  left: 0;
  width: 100%;
  height: 513rpx;
  background: linear-gradient(180deg, var(--primary-color) 0%, var(--primary-color-a) 100%);
  z-index: -1;
}

.section1 {
  position: relative;
  z-index: 3;
  /* margin: 0 24rpx; */
  padding: 16rpx 24rpx 22rpx;
  background: linear-gradient(180deg, var(--primary-color) 0%, #fe895a 100%);
}

.input-box {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}

.search {
  padding-left: 70rpx;
  position: relative;
  width: 702rpx;
  height: 68rpx;
  background: #FFFFFF;
  border-radius: 36rpx;
}

/* .search::before {
  position: absolute;
  left: 24rpx;
  top: 50%;
  transform: translateY(-50%);
  content: '';
  background-size: 100% 100%;
} */

.search::before {
  position: absolute;
  left: 24rpx;
  top: 50%;
  transform: translateY(-50%);
  content: '';
  background-size: 100% 100%;
  width: 32rpx;
  height: 30rpx;
  background-image: url("data:image/png;base64,");
}

.search-placeholder {
  font-size: 30rpx;
  color: #999999;
}

.btn-search {
  position: absolute;
  font-size: 26rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 56rpx;
  right: 30rpx;
  top: 21rpx;
  z-index: 2;
  width: 120rpx;
  height: 56rpx;
  background: linear-gradient(90deg, var(--primary-linear-start) 0%, var(--primary-linear-end) 100%);
  border-radius: 40rpx;
}

/* 轮播图 */
.section2 {}

.section2 swiper {
  width: 702rpx;
  height: 266rpx;
  margin: 0 auto;
  overflow: hidden;
  border-radius: 15rpx;
}

.section2 swiper-item {
  overflow: hidden;
  border-radius: 15rpx;
}

.section2 swiper .icon-banner {
  border-radius: 15rpx;
  width: 100%;
  height: 100%;
}

/* 分类列表 */
.section3 {
  margin: 22rpx 24rpx 0;
  box-shadow: 0rpx 1rpx 16rpx rgba(0, 0, 0, 0.1);
  border-radius: 16rpx;
  overflow: scroll;
  background: #FFFFFF;
}

.section3 .category-box {
  overflow: auto;
}

.section3 .category-box .l1 {
  display: flex;
}

.section3 .category-box .l2 {
  display: flex;
}

.section3 .item {
  padding-left: 3rpx;
  margin: 0 22rpx;
  padding: 22rpx 0 24rpx;
}

.section3 .l1 .item:last-child,
.section3 .l2 .item:last-child {
  padding-right: 22rpx;
}

.section3 .item .icon-category {
  width: 104rpx;
  height: 104rpx;
  background: rgba(0, 0, 0, 0);
  border-radius: 30rpx;
}

.section3 .item .text-category {
  margin-top: 16rpx;
  min-width: 96rpx;
  text-align: center;
  font-size: 25rpx;
  color: #666666;
}

.section4 {
  margin: 0 24rpx;
  font-size: 22rpx;
  color: #BBBBBB;
  display: flex;
  height: 72rpx;
  justify-content: space-between;
  align-items: center;
}

.section4 image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 4rpx;
  vertical-align: top;
}

/* 预收专栏 */
.section5 {
  margin: 0 24rpx;
}

.section5 .icon-presale {
  width: 702rpx;
  height: 220rpx;
}

.section5 .b {
  padding: 20rpx 24rpx;
  background: linear-gradient(180deg, #FFAB44 0%, #FFAB44 100%);
  border-radius: 0rpx 0rpx 16rpx 16rpx;
  display: flex;
  overflow: auto;
  /* justify-content: space-between; */
}

.section5 .item {
  padding: 8rpx 6rpx 10rpx;
  width: 152rpx;
  /* height: 234px; */
  background: #FFFFFF;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}

.section5 .item:not(:first-child) {
  margin-left: 17rpx;
}

.section5 .b .item .icon-presale {
  width: 140rpx;
  height: 140rpx;
}

.section5 .b .item .l2 {
  margin-top: 12rpx;
  width: 120rpx;
  height: 34rpx;
  line-height: 34rpx;
  background: var(--primary-color);
  border-radius: 0rpx 20rpx 0rpx 20rpx;
  font-size: 26rpx;
  font-weight: bold;
  color: #FFFFFF;
}

.section5 .b .item .l3 {
  margin-top: 8rpx;
  font-size: 22rpx;
  color: #CCCCCC;
  text-decoration: line-through;
}

/* 新品上市 */
.section6 {
  margin: 20rpx 24rpx 0;
  display: flex;
  background: #FFFFFF;
  border-radius: 16rpx;
}

.section6 .l {
  padding: 0 18rpx;
  width: 188rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

.section6 .l .l1 {
  margin-top: 52rpx;
